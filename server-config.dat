	"Booru.org (Gelbooru 0.1.11)":	{
					"getCount":"function Get-Count {
						param([String]$tags,[int]$limit=-1,[switch]$pages)
						if($tags -match '^(\\s|\\*)$'){ $tags='all' }
						if($pages){ return 20 }
						while($true){
							try{
								$page = Invoke-WebRequest -UseBasicParsing -Uri \"https://$($ssconfig.url)/index.php?page=post\u0026s=list\u0026tags=$tags\"
								break
							}catch [System.InvalidOperationException]{
								Start-Sleep -Milliseconds 500
								continue
							}
						}
						$thumbCount = ([regex]::Matches($page.Content, 'class=\"thumb\"')).Count
						if(($thumbCount -eq 0) -or ($limit -eq 0)){ return 0 }

						# Check for multi-page by finding paginator
						if($page.Content -match \"id=[`\"']paginator[`\"'][^>]*>([\\s\\S]*?)</div>\"){
							$paginator = $Matches[1]
							$pidMatches = [regex]::Matches($paginator, 'pid=(\\d+)')
							if($pidMatches.Count -gt 0){
								$maxPid = ($pidMatches | ForEach-Object { [int]$_.Groups[1].Value } | Measure-Object -Maximum).Maximum
								while($true){
									try{
										$lastPage = Invoke-WebRequest -UseBasicParsing -Uri \"https://$($ssconfig.url)/index.php?page=post\u0026s=list\u0026tags=$tags\u0026pid=$maxPid\"
										break
									}catch [System.InvalidOperationException]{
										Start-Sleep -Milliseconds 500
										continue
									}
								}
								$count = $maxPid + ([regex]::Matches($lastPage.Content, 'class=\"thumb\"')).Count
							}else{
								$count = $thumbCount
							}
						}else{
							$count = $thumbCount
						}
						if($count -gt $limit -and $limit -gt 0){ return $limit }
						return $count
						}",
					"getPost":"function Get-Post {
						param([int]$id,[switch]$raw)
						$response=$null
						while($response -eq $null){
							try{
								$response = Invoke-Webrequest -UseBasicParsing -Uri \"https://$($ssconfig.url)/index.php?page=post\u0026s=view\u0026id=$ID\"
							}catch [System.InvalidOperationException]{
								Start-Sleep -Milliseconds 500
								continue
							}
						}
						# Grab image URL
						if($response.Content -notmatch '<img[^>]+id=\"image\"[^>]+src=\"([^\"]+)\"'){
							if($response.Content -notmatch '<img[^>]+src=\"([^\"]+)\"[^>]+id=\"image\"'){ return $null }
						}
						$url = $Matches[1]
						if($raw){ return $response.Content }

						# Get tags from sidebar links
						$tagStr = ''
						if($response.Content -match 'id=\"tag_list\"[^>]*>([\\s\\S]*?)</ul>'){
							$tagMatches = [regex]::Matches($Matches[1], 'tags=([^\"&]+)')
							$tagStr = ($tagMatches | ForEach-Object { $_.Groups[1].Value }) -join ';'
						}
						$tagStr = $tagStr -replace ' ','_' -replace '%20','_'

						# Get image statistics
						$extraInfo = ''
						if($response.Content -match 'Statistics([\\s\\S]*?)</ul>'){
							$extraInfo = [System.Net.WebUtility]::HtmlDecode($Matches[1])
						}
						$uploadDate = [datetime]::MinValue
						if($extraInfo -match 'Posted:\\s*([^<\\r\\n]+)'){ try{ $uploadDate = [datetime]::Parse($Matches[1].Trim()) }catch{} }
						$size = @(0,0)
						if($extraInfo -match 'Size:\\s*(\\d+)\\s*x\\s*(\\d+)'){ $size = @($Matches[1], $Matches[2]) }
						$source = ''
						if($extraInfo -match 'Source:\\s*([^<\\r\\n]+)'){ $source = $Matches[1].Trim() }
						$score = 0
						if($extraInfo -match 'Score:\\s*(\\d+)'){ $score = [int]$Matches[1] }
						$rating = 'unknown'
						if($extraInfo -match 'Rating:\\s*(\\w+)'){ $rating = $Matches[1].ToLower() }

						#can't use MD5 here because the file URL contains SHA1(MD5(image)), and i would have to de-hash the URL (stupid and unnecessary)
						return @{id=$ID;file=$url;uploaded=$uploadDate;md5='SKIP';size=$size;source=$source;score=$score;tags=$tagStr;rating=$rating}
						}",
					"getTaggedPosts":"function Get-TaggedPosts {
						param([String]$tags,[switch]$raw,[int]$limit = -1,[switch]$count)
						if($tags -match '^(\\s|\\*)$'){ $tags='all' }

						$page = Invoke-WebRequest -UseBasicParsing -Uri \"https://$($ssconfig.url)/index.php?page=post\u0026s=list\u0026tags=$tags\"
						# Extract post IDs from thumb spans: <span class=\"thumb\"><a id=\"pNNN\">
						$idMatches = [regex]::Matches($page.Content, 'class=\"thumb\"[^>]*>[\\s]*<a[^>]*id=\"p(\\d+)\"')
						$postIDs = @($idMatches | ForEach-Object { $_.Groups[1].Value })
						$postArray = @()

						if(($postIDs.Count -eq 0) -or ($limit -eq 0)){ return $postArray }

						$postsPerPage = $postIDs.Count
						$totalPages = 1
						if($page.Content -match \"id=[`\"']paginator[`\"'][^>]*>([\\s\\S]*?)</div>\"){
							$pidMatches = [regex]::Matches($Matches[1], 'pid=(\\d+)')
							if($pidMatches.Count -gt 0){
								$maxPid = ($pidMatches | ForEach-Object { [int]$_.Groups[1].Value } | Measure-Object -Maximum).Maximum
								$totalPages = [Math]::floor($maxPid / $postsPerPage) + 1
							}
						}

						if($limit -gt 0){
							$pagesNeeded = [Math]::ceiling($limit / $postsPerPage)
							$totalPages = [Math]::Min($totalPages, $pagesNeeded)
						}

						$offset = 0
						$pageCounter = 0
						$totalFetched = 0

						do{
							$pageCounter++
							forEach($id in $postIDs){
								$postArray += Get-Post -id $id
								$totalFetched++
								if($totalFetched -eq $limit){break}
							}

							Write-Host \"Read page $pageCounter/$totalPages.\"
							if($totalFetched -eq $limit){break}

							$offset += $postIDs.Count
							while($true){
								try{
									$page = Invoke-WebRequest -UseBasicParsing -Uri \"https://$($ssconfig.url)/index.php?page=post\u0026s=list\u0026tags=$tags\u0026pid=$offset\"
									break
								}catch [System.InvalidOperationException]{
									Start-Sleep -Milliseconds 500
									continue
								}
							}
							$idMatches = [regex]::Matches($page.Content, 'class=\"thumb\"[^>]*>[\\s]*<a[^>]*id=\"p(\\d+)\"')
							$postIDs = @($idMatches | ForEach-Object { $_.Groups[1].Value })
						}while($postIDs.Count -gt 0)

						return $postArray
						}"
				}
}