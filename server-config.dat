						}else{
							$currentPage=(Invoke-WebRequest -UseBasicParsing -Uri \"$($ssconfig.url)/index.php?page=dapi\u0026s=post\u0026q=index\u0026tags=$tags\u0026pid=0\u0026limit=100$authStr\")
						}
						try{
							$currentPage = [xml]$currentPage
						} catch [System.Management.Automation.RuntimeException] {
