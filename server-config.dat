						}
						$counter=0
						$totalPages = if($limit -gt 0){[Math]::ceiling($limit/100)}else{[Math]::ceiling($currentPage.posts.count/100)}
						for ($i=1;$i -le $totalPages;$i++){
						  forEach($post in $currentPage.posts.post){
							$creationTime = [dateTime]::ParseExact($post.created_at,'ddd MMM dd HH:mm:ss zzz yyyy',$null)
							if($raw){
								$postArray += $post
							}else{
								$postArray += @{id=$post.id;file=$post.file_url;uploaded=$creationTime;md5=$post.md5;tags=$post.tags;rating=$post.rating;source=$post.source}
							}
							$counter++
							if($counter -eq $limit){break}
						  }

						  Write-Host $(\"Read page $i/$totalPages.\")
						  if($counter -eq $limit){break}
						  
						  $currentPage=(Invoke-WebRequest -UseBasicParsing -Uri \"$($ssconfig.url)/index.php?page=dapi\u0026s=post\u0026q=index\u0026tags=$tags\u0026pid=$i\u0026limit=100$authStr\")
						  try{
							$currentPage = [xml]$currentPage
						  } catch [System.Management.Automation.RuntimeException] {
						  	$currentPage = [xml][System.Net.WebUtility]::HtmlDecode($currentPage)
						  }
						}
						return $postArray
						}",
					 "getLogin": "function Get-Login{
						if($ssconfig.isAnonymous){
							return ''
						}
						$savedLoginData = @{}
						if($ssconfig.api_key -ne $null -and $ssconfig.user_id -ne $null){
							return \"\u0026api_key=$($ssconfig.api_key)\u0026user_id=$($ssconfig.user_id)\"
						}

						Write-Host \"| Gelbooru-type server selected. Some servers of this type require API credentials.`n| You can find your API key and User ID in your account settings on`n| the Gelbooru website under 'Settings' > 'Options' > 'API Access Credentials'.`n| `n| Would you like to enter your credentials now?`n| `n| WARNING: If you select 'Go Anonymous', this prompt will not show up again.`n| Pick 'Cancel' to postpone the choice until later.\"
						$loginChoice = Create-Menu -MenuTitle ' ' -MenuOptions ('Enter Credentials','Go Anonymous Forever','Cancel For Now') -DefaultSelection 0

						if($loginChoice -eq 1){
							$ssconfig.isAnonymous = $savedLoginData.isAnonymous=$True
							SaveTo-UserConfig -Members $savedLoginData
							return ''
						}elseif($loginChoice -eq 2){
							$ssconfig.isAnonymous = $True
							return ''
						}

						$userId = Read-Host -Prompt \"`nEnter User ID for $($ssconfig.url)\"
						$apiKey = Read-Host -Prompt \"Enter API key for $($ssconfig.url)\"

						$ssconfig.api_key = $savedLoginData.api_key = $apiKey
						$ssconfig.user_id = $savedLoginData.user_id = $userId
						$ssconfig.isAnonymous = $savedLoginData.isAnonymous = $False
						SaveTo-UserConfig -Members $savedLoginData

						return \"\u0026api_key=$apiKey\u0026user_id=$userId\"
					}"
                 },
	"Danbooru": {
                     "getCount":  "function Get-Count{
						param([String]$tags,[switch]$pages,[int]$limit=-1)
						if($pages){return 200}
						if($limit -eq 0){return 0}
						if($ssconfig.getLogin -ne $null){
							if(-not (test-path Function:/Get-Login)){
								Invoke-Expression $ssconfig.getLogin
							}
							$authStr = Get-Login
						}
						if($tags -eq ' '){
							$page = (Invoke-WebRequest -UseBasicParsing -UserAgent 'curl/7.68.0' -Uri \"https://$($ssconfig.url)/posts.json?limit=1$authStr\"|ConvertFrom-JSON)
							return $page[0].id
						}
						if($tags.length -eq 1 -or !$tags.substring(1,$tags.length-2).contains(\" \")){
							$tags = $tags.replace(' ','')
							$page = (Invoke-WebRequest -UseBasicParsing -UserAgent 'curl/7.68.0' -Uri \"https://$($ssconfig.url)/tags.json?search[name_like]=$tags\u0026limit=5$authStr\"|ConvertFrom-JSON)
							if($page.count -eq 0){
								return 0
							}
							if($page[0].name -eq $tags){
								if($limit -gt 0 -and $page[0].post_count -gt $limit){
									$count=$limit
								}else{
									$count=$page[0].post_count
								}
							}else{
								#possibly substitute incomplete tag for complete one thru create-menu?
								return 0
							}
						}else{
							$page = (Invoke-WebRequest -UseBasicParsing -UserAgent 'curl/7.68.0' -Uri \"https://$($ssconfig.url)/posts.json?limit=200\u0026tags=$tags$authStr\"|ConvertFrom-JSON)
							$count = 0
							$pageCounter = 2
							while($page[0] -ne $null){
								$count += $page.length
								$page = (Invoke-WebRequest -UseBasicParsing -UserAgent 'curl/7.68.0' -Uri \"https://$($ssconfig.url)/posts.json?limit=200\u0026tags=$tags\u0026page=$pageCounter$authStr\"|ConvertFrom-JSON)
								if($limit -gt 0 -and $count -gt $limit){
									$count=$limit
									break
								}
								$pageCounter++
							}
						}
						return $count
						}",
                     "getPost":  "function Get-Post{
						param([int]$id,[switch]$raw)
						if($ssconfig.getLogin -ne $null){
							if(-not (test-path Function:/Get-Login)){
								Invoke-Expression $ssconfig.getLogin
							}
							$authStr = Get-Login
						}
						try{
							$currentPage=(Invoke-WebRequest -UseBasicParsing -UserAgent 'curl/7.68.0' -Uri \"https://$($ssconfig.url)/posts/$id.json?1=1$authStr\"|ConvertFrom-JSON)
						}catch{
							Write-Error $Error[0].Exception.Message
							return $null
						}
						
						$post = $currentPage[0]
						if($post.id -ne $id){
							Write-Warning 'The post you were looking for could not be found. It was probably deleted.'
							return $null
						}
						
						if($raw){
							return $post
						}else{
							$date=[dateTime]::Parse($post.created_at)
							return @{id=$post.id;file=$post.file_url;uploaded=$date;md5=$post.md5;tags=($post.tag_string);rating=$post.rating;source=$post.source}
						}
						}",
                     "getTaggedPosts":  "function Get-TaggedPosts{
						param([String]$tags,[switch]$raw,[int]$limit=-1)
						if($ssconfig.getLogin -ne $null){
							if(-not (test-path Function:/Get-Login)){
								Invoke-Expression $ssconfig.getLogin
							}
							$authStr = Get-Login
						}
						$postArray = @()
						if($limit -lt 200 -and $limit -gt 0){
							$currentPage = (Invoke-WebRequest -UseBasicParsing -UserAgent 'curl/7.68.0' -Uri \"https://$($ssconfig.url)/posts.json?limit=$limit\u0026tags=$tags$authStr\"|ConvertFrom-JSON)
						}else{
							$currentPage = (Invoke-WebRequest -UseBasicParsing -UserAgent 'curl/7.68.0' -Uri \"https://$($ssconfig.url)/posts.json?limit=200\u0026tags=$tags$authStr\"|ConvertFrom-JSON)
						}
						if($currentPage[0] -eq $null -or $limit -eq 0){
							return @()
						}
						$counter=0
						$pageCounter=1
						do{
						  forEach($post in $currentPage){
							if($post.file_url -eq $null -and !$raw){
								continue
							}
							
							if($raw){
								$postArray += $post
							}else{
								$creationTime = $date=[dateTime]::Parse($post.created_at)
								$postArray += @{id=$post.id;file=$post.file_url;uploaded=$creationTime;md5=$post.md5;tags=($post.tag_string);rating=$post.rating;source=$post.source}
							}
							$counter++
							if($counter -eq $limit){break}
						  }

						  Write-Host \"Read page $pageCounter.\"
						  if($counter -eq $limit){break}
						  $pageCounter++

						  $currentPage=(Invoke-WebRequest -UseBasicParsing -UserAgent 'curl/7.68.0' -Uri \"https://$($ssconfig.url)/posts.json?limit=200\u0026tags=$tags\u0026page=$pageCounter$authStr\"|ConvertFrom-JSON)
						}while($currentPage[0] -ne $null)
						return $postArray
						}",
					 "getLogin": "function Get-Login{
						if($ssconfig.isAnonymous){
							return ''
						}
						$savedLoginData = @{}
						if($ssconfig.login -ne $null -and $ssconfig.api_key -ne $null){
							return \"\u0026login=$($ssconfig.login)\u0026api_key=$($ssconfig.api_key)\"
						}

						Write-Host \"| Danbooru-type server selected. This server may require API credentials.`n| You can find your API key in your account settings on the Danbooru`n| website under 'My Account' > 'API Key'.`n| `n| Would you like to enter your credentials now?`n| `n| WARNING: If you select 'Go Anonymous', this prompt will not show up again.`n| Pick 'Cancel' to postpone the choice until later.\"
						$loginChoice = Create-Menu -MenuTitle ' ' -MenuOptions ('Enter Credentials','Go Anonymous Forever','Cancel For Now') -DefaultSelection 0

						if($loginChoice -eq 1){
							$ssconfig.isAnonymous = $savedLoginData.isAnonymous=$True
							SaveTo-UserConfig -Members $savedLoginData
							return ''
						}elseif($loginChoice -eq 2){
							$ssconfig.isAnonymous = $True
							return ''
						}

						$username = Read-Host -Prompt \"`nEnter username for $($ssconfig.url)\"
						$apiKey = Read-Host -Prompt \"Enter API key for $($ssconfig.url)\"

						$ssconfig.login = $savedLoginData.login = $username
						$ssconfig.api_key = $savedLoginData.api_key = $apiKey
						$ssconfig.isAnonymous = $savedLoginData.isAnonymous = $False
						SaveTo-UserConfig -Members $savedLoginData

						return \"\u0026login=$username\u0026api_key=$apiKey\"
					}"
                 },
	"Shimmie":  {
                     "getCount":  "function Get-Count{
						param([String]$tags,[switch]$pages,[int]$limit=-1)
