							}else{
								$postTags = @()
								forEach($sub in $post.tags.PSObject.Properties){
									$postTags += $sub.value
								}
								$postArray += @{id=$post.id;file=$post.file.url;uploaded=$creationTime;md5=$post.file.md5;tags=($postTags -join ' ');rating=$post.rating;source=$post.source}
