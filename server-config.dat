						$page = (Invoke-WebRequest -UseBasicParsing -Uri \"$($ssconfig.url)/api/danbooru/find_posts?limit=70\u0026tags=$tags\") -replace '&#039;','&amp;#039;'
						try{
							$page = [xml]$page
						} catch [System.Management.Automation.RuntimeException] {
							$page = [xml]([System.Net.WebUtility]::HtmlDecode($page) -replace '&','&amp;')
						}
						if($limit -gt 0 -and $page.posts.count -gt $limit){return $limit}
